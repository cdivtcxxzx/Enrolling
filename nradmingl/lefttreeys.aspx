<script>
    $(function () {
        InitialPage();
        GetTree();
        GetGrid();
    });
    //初始化页面
    function InitialPage() {
        //layout布局
        $('#layout').layout({
            applyDemoStyles: true,
            onresize: function () {
                $(window).resize();
            }
        });
        //resize重设(表格、树形)宽高
        $(window).resize(function (e) {
            window.setTimeout(function () {
                $('#gridTable').setGridWidth(($('.gridPanel').width()));
                $("#gridTable").setGridHeight($(window).height() - 141);
                $("#itemTree").setTreeHeight($(window).height() - 52);
            }, 200);
            e.stopPropagation();
        });
    };
    //加载树
    var AreaCode = 0;
    function GetTree() {
        var item = {
            height: $(window).height() - 52,
            url: "../../SystemManage/Area/GetTreeJson",
            onnodeclick: function (item) {
                AreaCode = item.id;
                //展开下级
                $(".bbit-tree-selected").children('.bbit-tree-ec-icon').trigger("click");
                $('#btn_Search').trigger("click");
            },
        };
        //初始化
        $("#itemTree").treeview(item);
    };
    //加载表格
    function GetGrid() {
        var selectedRowIndex = 0;
        var $gridTable = $("#gridTable");
        $gridTable.jqGrid({
            url: "../../SystemManage/Area/GetListJson",
            datatype: "json",
            height: $(window).height() - 141,
            autowidth: true,
            colModel: [
                { label: '主键', name: 'F_AreaId', hidden: true },
                { label: '编号', name: 'F_AreaCode', index: 'F_AreaCode', width: 100, align: 'left' },
                { label: '名称', name: 'F_AreaName', index: 'F_AreaName', width: 300, align: 'left' },
                { label: '简拼', name: 'F_SimpleSpelling', index: 'F_SimpleSpelling', width: 100, align: 'left' },
                { label: '级别', name: 'F_Layer', index: 'F_Layer', width: 50, align: 'center' },
                {
                    label: "有效", name: "F_EnabledMark", index: "F_EnabledMark", width: 50, align: "center",
                    formatter: function (cellvalue, options, rowObject) {
                        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                    }
                },
                { label: "备注", name: "F_Description", index: "F_Description", width: 200, align: "left" }
            ],
            rowNum: "10000",
            rownumbers: true,
            onSelectRow: function () {
                selectedRowIndex = $("#" + this.id).getGridParam('selrow');
            },
            gridComplete: function () {
                $("#" + this.id).setSelection(selectedRowIndex, false);
            }
        });
        //查询事件
        $("#btn_Search").click(function () {
            $gridTable.jqGrid('setGridParam', {
                postData: { value: AreaCode, keyword: $("#txt_Keyword").val() },
            }).trigger('reloadGrid');
        });
    };
    //新增
    function btn_add() {
        var AreaId = AreaCode;
        $.learunUIBase.dialogOpen({
            id: "Form",
            title: '添加区域',
            url: '/SystemManage/Area/Form?parentId=' + AreaId,
            width: "500px",
            height: "310px",
            callBack: function (iframeId) {
                top.frames[iframeId].AcceptClick();
            }
        });
    };
    //编辑
    function btn_edit() {
        var keyValue = $("#gridTable").jqGridRowValue("F_AreaId");
        if ($.learunUIBase.checkedRow(keyValue)) {
            $.learunUIBase.dialogOpen({
                id: "Form",
                title: '编辑区域',
                url: '/SystemManage/Area/Form?keyValue=' + keyValue,
                width: "500px",
                height: "310px",
                callBack: function (iframeId) {
                    top.frames[iframeId].AcceptClick();
                }
            });
        }
    };
    //删除
    function btn_delete() {
        var keyValue = $("#gridTable").jqGridRowValue("F_AreaId");
        if (keyValue) {
            $.learunUIBase.removeForm({
                url: "../../SystemManage/Area/RemoveForm",
                param: { keyValue: keyValue },
                success: function (data) {
                    $("#gridTable").trigger("reloadGrid");
                }
            });
        } else {
            $.learunUIBase.dialogMsg({ msg: '请选择需要删除的区域！', type: 0 });
        }
    };
    //详细
    function btn_detail() {
        var keyValue = $("#gridTable").jqGridRowValue("F_AreaId");
        if ($.learunUIBase.checkedRow(keyValue)) {
            $.learunUIBase.dialogOpen({
                id: "Detail",
                title: '区域信息',
                url: '/SystemManage/Area/Detail?keyValue=' + keyValue,
                width: "500px",
                height: "430px",
                btn: null
            });
        }
    };
</script>
